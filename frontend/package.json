{
  "name": "audioseva-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "jest",
    "test:watch": "jest --watch --noStackTrace --detectOpenHandles",
    "lint": "vue-cli-service lint",
    "deploy": "firebase deploy --only hosting",
    "serve:storybook": "start-storybook -p 6006",
    "test:unit": "npm run test"
  },
  "engines": {
    "node": "18.18.2"
  },
  "dependencies": {
    "@casl/ability": "4.1.6",
    "@casl/vue": "1.2.3",
    "bluebird": "3.7.2",
    "core-js": "3.33.3",
    "firebase": "8.10.1",
    "firebaseui": "4.8.1",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "moment-timezone": "0.5.43",
    "p-queue": "6.6.2",
    "spark-md5": "3.0.2",
    "vue": "2.7.15",
    "vue-class-component": "7.2.6",
    "vue-page-title": "1.3.0",
    "vue-property-decorator": "9.1.2",
    "vue-resource": "1.5.3",
    "vue-router": "3.6.5",
    "vue2-dropzone": "3.6.0",
    "vuefire": "2.2.5",
    "vuetify": "2.7.1",
    "vuex": "3.6.2",
    "whatwg-fetch": "3.6.19"
  },
  "devDependencies": {
    "@babel/preset-react": "7.14.5",
    "@mdi/font": "5.9.55",
    "@storybook/addon-a11y": "5.3.21",
    "@storybook/addon-actions": "5.3.21",
    "@storybook/addon-knobs": "5.3.21",
    "@storybook/addon-notes": "5.3.21",
    "@storybook/addon-viewport": "5.3.21",
    "@storybook/addons": "5.3.21",
    "@storybook/vue": "5.3.21",
    "@types/bluebird": "3.5.36",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.175",
    "@types/moment-timezone": "0.5.13",
    "@types/spark-md5": "3.0.2",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "@vue/cli-plugin-babel": "4.5.19",
    "@vue/cli-plugin-eslint": "4.5.19",
    "@vue/cli-plugin-typescript": "4.5.19",
    "@vue/cli-plugin-unit-jest": "4.5.19",
    "@vue/cli-service": "4.5.19",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "6.0.0",
    "@vue/test-utils": "1.3.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-preset-vue": "2.0.2",
    "cache-loader": "4.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-vue": "6.2.2",
    "eslint-plugin-vuetify": "1.1.0",
    "flush-promises": "1.0.2",
    "fork-ts-checker-webpack-plugin": "6.5.3",
    "jest": "26.6.3",
    "jest-serializer-vue": "2.0.2",
    "prettier": "2.8.8",
    "prism-react-renderer": "1.3.5",
    "prismjs": "1.29.0",
    "storybook-vue-router": "1.0.7",
    "ts-jest": "26.5.6",
    "typescript": "4.9.5",
    "vue-cli-plugin-vuetify": "2.5.8",
    "vue-cli-plugin-vuetify-storybook": "0.2.1",
    "vue-jest": "3.0.7",
    "vue-storybook": "1.2.0",
    "vue-template-compiler": "2.7.15"
  },
  "overrides": {
    "css-loader": "3.6.0",
    "react-dom": "16.14.0",
    "terser-webpack-plugin": "2.3.8",
    "webpack": "5.94.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/unit/setupTests.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
