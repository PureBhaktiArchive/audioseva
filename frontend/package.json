{
  "name": "audioseva-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "deploy": "firebase deploy --only hosting",
    "serve:storybook": "start-storybook -p 6006",
    "test:unit": "npm run test"
  },
  "dependencies": {
    "@babel/polyfill": "7.7.0",
    "firebase": "7.5.0",
    "firebaseui": "4.3.0",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "moment-timezone": "0.5.27",
    "vue": "2.6.10",
    "vue-class-component": "7.1.0",
    "vue-property-decorator": "8.3.0",
    "vue-resource": "1.5.1",
    "vue-router": "3.1.3",
    "vue2-dropzone": "3.6.0",
    "vuefire": "2.2.0",
    "vuetify": "2.1.11",
    "vuex": "3.1.2",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "7.7.4",
    "@mdi/font": "4.6.95",
    "@storybook/addon-a11y": "5.2.6",
    "@storybook/addon-actions": "5.2.6",
    "@storybook/addon-knobs": "5.2.6",
    "@storybook/addon-notes": "5.2.6",
    "@storybook/addon-viewport": "5.2.6",
    "@storybook/addons": "5.2.6",
    "@storybook/vue": "5.2.6",
    "@types/jest": "24.0.23",
    "@types/lodash": "4.14.149",
    "@types/moment-timezone": "0.5.12",
    "@types/node": "10.14.19",
    "@typescript-eslint/eslint-plugin": "2.9.0",
    "@typescript-eslint/parser": "2.9.0",
    "@vue/cli-plugin-babel": "4.0.5",
    "@vue/cli-plugin-eslint": "4.0.5",
    "@vue/cli-plugin-typescript": "4.0.5",
    "@vue/cli-plugin-unit-jest": "4.0.5",
    "@vue/cli-service": "4.0.5",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "5.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-preset-vue": "2.0.2",
    "eslint": "6.7.1",
    "eslint-plugin-vue": "6.0.1",
    "eslint-plugin-vuetify": "1.0.0-beta.5",
    "flush-promises": "1.0.2",
    "fork-ts-checker-webpack-plugin": "3.1.1",
    "jest-serializer-vue": "2.0.2",
    "lint-staged": "9.4.3",
    "node-sass": "4.13.0",
    "prism-react-renderer": "1.0.2",
    "prismjs": "1.17.1",
    "sass-loader": "8.0.0",
    "storybook-vue-router": "1.0.7",
    "ts-jest": "24.2.0",
    "typescript": "3.7.2",
    "vue-cli-plugin-vuetify": "2.0.2",
    "vue-cli-plugin-vuetify-storybook": "0.1.1",
    "vue-jest": "3.0.5",
    "vue-storybook": "1.1.0",
    "vue-template-compiler": "2.6.10"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/unit/setupTests.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
