{
  "database": {
    "rules": "./database.rules.json"
  },
  "storage": [{
      "target": "se_uploads",
      "rules": "./functions/storage.uploads.rules"
  }],
  "hosting": {
    "public": "frontend/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {}
}
